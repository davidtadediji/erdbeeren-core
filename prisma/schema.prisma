generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id               Int      @id @default(autoincrement())
  username         String
  password         String
  email            String
  role             String
  verificationCode String
  isVerified       Boolean    @default(false)
}


